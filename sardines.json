{
    "services": [
        {
            "name": "setup",
            "module": "/repository",
            "arguments": [
                {
                    "name": "settings",
                    "type": "RepositorySettings"
                }
            ],
            "returnType": "any",
            "isAsync": true
        },
        {
            "name": "signIn",
            "module": "/repository",
            "arguments": [
                {
                    "name": "account",
                    "type": "Account"
                },
                {
                    "name": "password",
                    "type": "string"
                }
            ],
            "returnType": "string",
            "isAsync": true
        },
        {
            "name": "signOut",
            "module": "/repository",
            "arguments": [
                {
                    "name": "token",
                    "type": "string"
                }
            ],
            "returnType": "any",
            "isAsync": true
        },
        {
            "name": "signUp",
            "module": "/repository",
            "arguments": [
                {
                    "name": "account",
                    "type": "Account"
                },
                {
                    "name": "password",
                    "type": "string"
                }
            ],
            "returnType": "any",
            "isAsync": true
        },
        {
            "name": "createOrUpdateApplication",
            "module": "/repository",
            "arguments": [
                {
                    "name": "application",
                    "type": "Application"
                },
                {
                    "name": "token",
                    "type": "string"
                }
            ],
            "returnType": "any",
            "isAsync": true
        },
        {
            "name": "queryApplication",
            "module": "/repository",
            "arguments": [
                {
                    "name": "application",
                    "type": "Application|{id"
                },
                {
                    "name": "token",
                    "type": "string"
                }
            ],
            "returnType": "any",
            "isAsync": true
        },
        {
            "name": "deleteApplication",
            "module": "/repository",
            "arguments": [
                {
                    "name": "application",
                    "type": "Application"
                },
                {
                    "name": "token",
                    "type": "string"
                }
            ],
            "returnType": "any",
            "isAsync": true
        },
        {
            "name": "queryService",
            "module": "/repository",
            "arguments": [
                {
                    "name": "service",
                    "type": "Service"
                },
                {
                    "name": "token",
                    "type": "string"
                }
            ],
            "returnType": "Service|null",
            "isAsync": true
        },
        {
            "name": "createOrUpdateService",
            "module": "/repository",
            "arguments": [
                {
                    "name": "service",
                    "type": "Service"
                },
                {
                    "name": "token",
                    "type": "string"
                }
            ],
            "returnType": "Service|null",
            "isAsync": true
        },
        {
            "name": "deleteService",
            "module": "/repository",
            "arguments": [
                {
                    "name": "service",
                    "type": "Service"
                },
                {
                    "name": "token",
                    "type": "string"
                }
            ],
            "returnType": "any",
            "isAsync": true
        }
    ]
}