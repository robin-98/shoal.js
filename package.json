{
    "author": {
        "email": "robin@naturewake.com",
        "name": "Robin Sun"
    },
    "bin": {
        "manage-sardines-repo": "lib/manager/manageRepository.js",
        "start-sardines-repo": "lib/index.js"
    },
    "bugs": {
        "url": "https://github.com/robin-98/sardines.shoal.http.js/issues"
    },
    "dependencies": {
        "@types/node": "^12.6.9",
        "@types/semver": "^6.0.1",
        "@types/systeminformation": "^3.23.1",
        "sardines-built-in-services": "*",
        "sardines-compile-time-tools": "*",
        "sardines-core": "*",
        "semver": "^6.3.0",
        "systeminformation": "^4.14.4",
        "typescript": "^3.5.3"
    },
    "description": "A built-in service provider for sardines.js",
    "devDependencies": {},
    "homepage": "https://github.com/robin-98/sardines.shoal.http.js#readme",
    "keywords": [
        "Free-to-Decompose",
        "distributed",
        "framework",
        "FaaS"
    ],
    "license": "ISC",
    "main": "lib/index.js",
    "name": "sardines-shoal",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robin-98/sardines.shoal.http.js.git"
    },
    "scripts": {
        "build": "npm run compileRepo && npm run reverseCompile && npm run publishRepositoryInterfaces && npm run clear && tsc --build conf/tsconfig.json && npm run makeExec",
        "clear": "rm -rf lib/*",
        "compileRepo": "file=repository.json; rm -f $file && sardines-compiler --application=sardines --recompile --verbose --gen-services=$file ./src/repository/index.ts",
        "makeExec": "npm run makeExecRepo && npm run makeExecManRepo",
        "makeExecManRepo": "file=lib/manager/manageRepository.js; { echo '#!/usr/bin/env node'; cat $file; } > ${file}.tmp && mv ${file}.tmp $file && chmod a+x $file",
        "makeExecRepo": "file=lib/index.js; { echo '#!/usr/bin/env node'; cat $file; } > ${file}.tmp && mv ${file}.tmp $file && chmod a+x $file",
        "reverseCompile": "sardines-compiler --reverse ./src",
        "publishRepositoryInterfaces": "file=./bin/publish_repository_service_definitions_to_core_project.py; if [ -f $file ];then $file repository.json ../sardines.core.js/src/repository_services.ts; fi",
        "start": "npm run build && node lib/index.js",
        "test": "npm run build && node lib/agent/host_perf.js test",
        "testDeployHost": "./bin/deploy_host.py --host-name nw-dev01 --os-user rin --repo-deploy-file deploy-repository-dev.json"
    },
    "type": "lib/index.d.ts",
    "version": "0.1.3"
}
